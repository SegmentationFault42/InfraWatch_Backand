{
    "name": "infrawatch-backend",
    "version": "1.0.0",
    "main": "src/app.js",
    "type": "commonjs",
    "scripts": {
        "build": "prisma generate && prisma generate --schema=prisma/timeseries.prisma && tsc",
        "start": "node dist/app.js",
        "dev": "tsx watch src/app.ts",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "format": "prettier --write .",
        "test": "vitest run",
        "test:watch": "vitest",
        "prisma:generate": "prisma generate && prisma generate --schema=prisma/timeseries.prisma",
        "prisma:push": "prisma db push && prisma db push --schema=prisma/timeseries.prisma",
        "prisma:studio": "prisma studio",
        "prisma:studio-ts": "prisma studio --schema=prisma/timeseries.prisma --port 5556",
        "prisma:migrate": "prisma migrate dev && prisma migrate dev --schema=prisma/timeseries.prisma",
        "db:test": "tsx src/scripts/test-databases.ts",
        "db:seed": "tsx src/scripts/seed-metrics.ts"
    },
    "dependencies": {
        "@fastify/cookie": "^11.0.2",
        "@fastify/rate-limit": "^10.3.0",
        "@fastify/secure-session": "^8.2.0",
        "@fastify/swagger": "^9.5.1",
        "@fastify/swagger-ui": "^5.2.3",
        "@prisma/client": "^6.15.0",
        "axios": "^1.11.0",
        "bcrypt": "^6.0.0",
        "bullmq": "^5.58.4",
        "dotenv": "^17.2.1",
        "fastify": "^5.4.0",
        "jsonwebtoken": "^9.0.2",
        "net-snmp": "^3.25.0",
        "pg": "^8.16.3",
        "ping": "^0.4.4",
        "snmp": "^0.0.3",
        "zod": "^4.0.17"
    },
    "devDependencies": {
        "@eslint/js": "^9.33.0",
        "@fastify/autoload": "^6.3.1",
        "@fastify/type-provider-typebox": "^5.2.0",
        "@types/bcrypt": "^6.0.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^24.3.0",
        "@types/ping": "^0.4.4",
        "@types/supertest": "^6.0.3",
        "@typescript-eslint/eslint-plugin": "^8.39.0",
        "@typescript-eslint/parser": "^8.39.0",
        "eslint": "^9.33.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "prettier": "^3.6.2",
        "prisma": "^6.14.0",
        "supertest": "^7.1.4",
        "tsx": "^4.20.5",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.39.0",
        "vitest": "^3.2.4"
    }
}
